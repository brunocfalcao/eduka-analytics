{
    "name": "brunocfalcao/eduka-analytics",
    "description": "Eduka analytics",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/eduka-analytics",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "brunocfalcao/eduka-abstracts": "dev-master",
        "brunocfalcao/cerebrus": "dev-master",
        "jaybizzle/crawler-detect": "^1.2",
        "illuminate/contracts": "^9.0",
        "jenssegers/agent": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\Analytics\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-eduka-analytics": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\Analytics\\AnalyticsServiceProvider"
            ]
        }
    }
}
